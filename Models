Training <- read.csv('https://raw.githubusercontent.com/telamparo/ECON-386-Group-Project/master/Training%20Data.csv', header = TRUE)
View(Training)
dim(Training)
cor(Training[,7:11])
cor(Training$price, Training$bathrooms)
cor(Training$price, Training$accomodates)
cor(Training$price, Training$beds)
cor(Training$price, Training$number_of_reviews)
cor(Training$price, Training$review_scores_rating)
M1 <- lm(price~accommodates+bathrooms+bedrooms+beds+number_of_reviews+review_scores_rating+neighbourhood, Training) #kind of including everything model, nothing stands out
m2<- lm(price~ accommodates+bathrooms+beds, Training)
summary(m2) ##Important  variables, price to how many people can be there and in how many beds
m3<-lm(price~bedrooms+bathrooms, Training)
summary(m3) ##very similiar, takes out the accomodate variable
plot(m3$residuals)
RMSE=sqrt(sum((predictions-Training$price)^2)/(length(Training$price)-3))
